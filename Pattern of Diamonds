#include<stdio.h>
int main(){
    int rows,i,j;
    char x='A';
    printf("KIRTIVARDHAN AJMANI");
    printf("\n enter the number of rows");
    scanf("%d",&rows);
    for(i=1;i<=rows;i++){
        for(j=1;j<=i;j++){
            x++;
            printf("%c",x);
        }
     printf("\n");   
    }
    return 0;
}

