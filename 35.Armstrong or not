#include <stdio.h>

int main()
{int num,c,arm=0,r;
    printf("KIRTIVARDHAN AJMANI");
    printf("\n Enter the value");
    scanf("%d",&num);//153
    c=num;//c=153
    while(num>0){
    r=num%10;
    arm=(r*r*r)+arm;
    num=num/10;
    }
    if(c==arm){
        printf("The number is armstrong");
    }
    else
    {
        printf("The number is not armstrong");
    }
    return 0;
}

